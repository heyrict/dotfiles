{
  "diagnostic": {
    "warningSign": "!>",
    "errorSign": ">>",
    "infoSign": "i>",
    "hintSign": "*>"
  },
  "list": {
    "nextKeymap": "<C-N>",
    "previousKeymap": "<C-P>"
  },
  "coc.preferences": {
    "silentAutoupdate": false,
    "extensionUpdateCheck": "never",
    "formatOnSaveFiletypes": [
      "python",
      "javascript",
      "javascript.jsx",
      "typescript",
      "typescript.tsx",
      "vue",
      "rust",
      "dart",
      "html",
      "json",
      "yaml"
    ]
  },
  "prettier.disableLanguages": ["vue", "markdown", "rust", "python"],
  "tsserver.npm": "~/.nvm/versions/node/v12.16.3/bin/npm",
  "tsserver.enablejavascript": false,
  "rust-analyzer.serverPath": "~/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu/bin/rust-analyzer",
  "python": {
    "jediEnabled": false
  },
  "sql.database": "postgresql",
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": [
        "~/Flutter/bin/cache/dart-sdk/bin/snapshots/analysis_server.dart.snapshot",
        "--lsp"
      ],
      "filetypes": ["dart"],
      "disableDynamicRegister": true,
      "trace.server": "verbose"
    },
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmPath": "elm",
        "elmFormatPath": "elm-format",
        "elmTestPath": "elm-test",
        "elmAnalyseTrigger": "change"
      }
    }
  }
}
